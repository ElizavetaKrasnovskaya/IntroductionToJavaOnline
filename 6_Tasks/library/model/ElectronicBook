package model;

public class ElectronicBook extends Book {

    private String name;
    private String author;
    private String annotation;
    private String yearOfPublishing;
    private final static String type = "electronic book";

    public ElectronicBook(String name, String author, String yearOfPublishing, String annotation){
    
        this.name = name;
        this.author = author;
        this.yearOfPublishing = yearOfPublishing;
        this.annotation = annotation;
        
    }

    public String toString(){
    
        String line = name + " " + author + " " + type + " " + yearOfPublishing + " " + annotation;
        
        return line;
        
    }
}
