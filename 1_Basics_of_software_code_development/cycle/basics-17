package by.jonline.basics17.main;

public class Main {

    // Вывести на экран соответствий между символами и их численными обозначениямив памяти компьютера
    
    public static void main(String[] args) {
    
	    for(int i=0; i<256; i++){
      
	        char symbol = (char)i;
          System.out.println(i + " " + symbol);
          
        }
    }
}
