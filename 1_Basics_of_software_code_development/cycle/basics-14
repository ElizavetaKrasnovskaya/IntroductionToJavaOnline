package by.jonline.basics14.main;

public class Main {

    // Найдите сумму квадратов первых 100 чисел
    
    public static void main(String[] args) {
    
        int result = 0;
	      for(int i =1; i<=100;i++){
        
	        result += Math.pow(i,2);
          
        }
        
        System.out.println("The sum of the squares of the first hundred numbers is " + result);
    }
}
