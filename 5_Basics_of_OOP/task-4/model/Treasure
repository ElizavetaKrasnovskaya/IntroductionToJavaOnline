package model;

public class Treasure {

    private String name;
    private int cost;

    public Treasure(){
    
        name = "";
        cost = 0;
        
    }

    public int getCost() {
    
        return cost;
        
    }

    public String getName() {
    
        return name;
        
    }

    public String toString(){
    
        String line = "";
        line += getName() + "\t" + getCost();
        return line;
        
    }
}
