package by.jonline.class2.test2;

public class Test2 {

    private int a;
    private int b;
    
    public Test2(int A, int B){
    
        a = A;
        b = B;
        
    }
    public Test2(){
    
        a = 0;
        b = 0;
        
    }

    public int get_a(){
    
        return a;
        
    }
    public int get_b(){
    
        return b;
        
    }
    public void set_a(int A){
    
        a = A;
        
    }
    public void set_b(int B){
    
        b = B;
        
    }
}
